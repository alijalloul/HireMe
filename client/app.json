{
  "expo": {
    "name": "HireMe",
    "slug": "HireMe",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "androidStatusBar": {
      "backgroundColor": "#FFFFFF",
      "barStyle": "dark-content" 
    },
    "notification": {
      "icon": "./assets/icon.png",
      "color": "#FBF2E3"
    },
    "userInterfaceStyle": "light",
