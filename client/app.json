      "package": "com.hireme.alex",
      "versionCode": 1,
      "googleServicesFile": "./google-services.json"
    },
    "web": {
      "favicon": "./assets/favicon.png",
      "backgroundColor": "#FBF2E3",
      "bundler": "metro"
    },
    "plugins": [

    ],
    "extra": {
      "eas": {
        "projectId": "0cd648b5-7923-4429-939a-3b2ca6a7bef0"
      }
    }
  }
